{
    "headers": [
        {
            "source": "/webhook/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET,POST,PUT,DELETE,OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "*"
                }
            ]
        },
        {
            "source": "/video-proxy/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        },
        {
            "source": "/media-proxy/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        },
        {
            "source": "/generations-proxy/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/webhook/:path*",
            "destination": "https://n8n.simplifies.click/:path*"
        },
        {
            "source": "/video-proxy/:path*",
            "destination": "https://nyc3.digitaloceanspaces.com/:path*"
        },
        {
            "source": "/media-proxy/:path*",
            "destination": "https://media-catalog.nyc3.digitaloceanspaces.com/:path*"
        },
        {
            "source": "/generations-proxy/:path*",
            "destination": "https://video-generations.nyc3.digitaloceanspaces.com/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}